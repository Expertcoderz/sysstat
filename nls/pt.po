#: sar.c:159
#, c-format
msgid "\t-F [ MOUNT ]\n"
msgstr "\t-F [ MOUNT ]\n"

#: sar.c:160
#, c-format
msgid "\t\tFilesystems statistics [A_FS]\n"
msgstr "\t\tEstatísticas do sistema de ficheiros [A_FS]\n"

#: sar.c:161
#, c-format
