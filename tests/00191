# Also include ill-formed strings ("C=34|21", "a=", "C=33;22;153;12;134;156;211")
LC_ALL=C TZ=GMT S_TIME_FORMAT= S_COLORS=always S_COLORS_SGR="C=33;22:C=34|21:X=31;1:a=:I=32;22:C=33;22;153;12;134;156;211:W=35;1:N=34;1:R=31;22:Z=36;22" ./sar -q MEM -m BAT -Bu -P 0,3 -d --dev=sda --getenv -f tests/data-new.tmp > tests/out1.sar-always.tmp && diff -u ${T_SRCDIR}/tests/expected1.sar-always tests/out1.sar-always.tmp
